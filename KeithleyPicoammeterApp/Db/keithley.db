record(aai, "$(P)$(R)Acquire")
{
    field(DESC, "Start Acquiring data")
    field(DTYP, "stream")
    field(INP, "@keithley.proto startAcquisition($(P)$(R)NumSampleSteps) $(PORT)")
    field(NELM, "1")
    #field(SCAN, "passive")
    #field(PREC, "6")
    field(FTVL, "FLOAT")
}

record(ai, "$(P)$(R)NumSampleSteps")
{
    field(DESC, "Number Of Sample Steps")
    field(LOPR, "1")
    field(HOPR, "1000")
    field(VAL, "1")
}

record(ao, "$(P)$(R)TimePerSampleStep")
{
    field(DESC, "Time Per Sample Step")
    field(PREC, "7")
    field(VAL, "0.131037")
    field(SCAN, "1 second")
    field(FLNK, "$(P)$(R)TimePerSampleStepRBV")
}

record(ai, "$(P)$(R)TimePerSampleStepRBV")
{
    field(DESC, "Time Per Sample Step")
    field(PREC, "7")
    field(SCAN, "1 second")
    field(INP, "$(P)$(R)TimePerSampleStep")
    field(PINI, "YES")
    #field(FLNK, "$(P)$(R)Acquire")
}

record(transform, "$(P)$(R)CalcTimePerSampleStep")
{
    field(DESC, "Calc Time Per Sample Step")
    field(INPA, "$(P)$(R)AutoZeroRBV CP")
    field(INPB, "$(P)$(R)IntegrationTimeRBV CP")
    field(INPC, "$(P)$(R)AverageFilterCountRBV")
    field(INPD, "$(P)$(R)AverageFilterEnableRBV CP")
    field(CLCE, "A ? 3 : 1")
    field(CLCF, "D ? C * 1.0285 : 1")
    field(CLCG, "E * (B / 50) * F + 0.0076172")
    field(OUTG, "$(P)$(R)TimePerSampleStep")
}

record(transform, "$(P)$(R)CalcNumberOfSamplesPerStep")
{
    field(DESC, "Calc number of samples per step")
    field(INPA, "$(P)$(R)AutoZeroRBV CP")
    field(INPB, "$(P)$(R)IntegrationTimeRBV CP")
    field(INPC, "$(P)$(R)TimePerSampleStep CP")
    field(CLCD, "A ? 3 : 1")
    field(CLCE, "((C-0.0076162) * 50) / (D * B * 1.0285)")
    field(OUTE, "$(P)$(R)AverageFilterCount")
    field(OUTF, "$(P)$(R)AverageFilterCount.PROC 1")
}

